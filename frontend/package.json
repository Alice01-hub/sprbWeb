{
  "name": "sprb-web",
  "private": true,
  "version": "1.0.0",
  "description": "Summer Pockets 圣地巡礼网站",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:progress": "vite --config vite.progress.config.ts",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "build:progress": "tsc && vite build --config vite.progress.config.ts",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "preview:progress": "vite preview --config vite.progress.config.ts",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "analyze": "npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.10.0",
    "framer-motion": "^10.16.4",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.15.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
